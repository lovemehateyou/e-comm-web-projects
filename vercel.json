{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" }, 
    { "src": "/styles/(.*)", "dest": "/styles/$1" },  
    { "src": "/scripts/(.*)", "dest": "/scripts/$1" }, 
    { "src": "/images/(.*)", "dest": "/images/$1" },  
    { "src": "/image/(.*)", "dest": "/image/$1" },  
    { "src": "/(.*)", "dest": "/index.html" }

  ]
}
